input[type="radio"].FormControl-radio::before { clip-path: circle(0% at 50% 50%); -webkit-mask-image: url("data:image/svg+xml;base64,"); -webkit-mask-size: 65%; }

@media screen and (prefers-reduced-motion: no-preference) {
  input[type="radio"].FormControl-radio::before { animation: 80ms cubic-bezier(0.65, 0, 0.35, 1) 0s 1 normal forwards running radioOut; }
}

@media screen and (prefers-reduced-motion: no-preference) {
  input[type="radio"].FormControl-radio:checked::before { animation: 80ms cubic-bezier(0.65, 0, 0.35, 1) 80ms 1 normal forwards running radioIn; }
}

input[type="radio"].FormControl-radio:indeterminate::before { visibility: hidden; }

@keyframes checkmarkIn { 
  0% { clip-path: inset(var(--base-size-16, 16px) 0 0 0); }
  100% { clip-path: inset(0px); }
}

@keyframes checkmarkOut { 
  0% { clip-path: inset(0px); }
  100% { clip-path: inset(var(--base-size-16, 16px) 0 0 0); }
}

@keyframes radioIn { 
  0% { clip-path: circle(0% at 50% 50%); }
  100% { clip-path: circle(100% at 50% 50%); }
}

@keyframes radioOut { 
  0% { clip-path: circle(100% at 50% 50%); }
  100% { clip-path: circle(0% at 50% 50%); }
}

